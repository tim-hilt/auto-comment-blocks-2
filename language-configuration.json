{
  "onEnterRules": [
    {
      "beforeText": "/\\*",
      "afterText": "*/",
      "action": {
        "indent": "indentOutdent",
        "appendText": " * "
      }
    },
    {
      "beforeText": "^.*\\*",
      "action": {
        "indent": "none",
        "appendText": "* "
      }
    }
  ]
}
